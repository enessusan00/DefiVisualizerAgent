{
  "name": "defi-visualizer",
  "version": "1.0.0",
  "description": "Automatic DeFi data visualization and infographic generator using OpenServ SDK",
  "main": "dist/index.js",
  "scripts": {
    "start": "ts-node src/index.ts",
    "dev": "ts-node-dev --respawn src/index.ts",
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "test": "jest",
    "cli": "ts-node src/cli.ts",
    "example:market": "ts-node src/examples/market-overview-example.ts",
    "example:price": "ts-node src/examples/price-chart-example.ts",
    "example:social": "ts-node src/examples/social-distribution-example.ts",
    "example:agent": "ts-node src/examples/agent-example.ts",
    "example:all": "ts-node src/cli.ts all"
  },
  "keywords": [
    "defi",
    "visualization",
    "infographics",
    "openserv",
    "crypto"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@openserv-labs/sdk": "^1.0.0",
    "axios": "^1.6.2",
    "chart.js": "^4.4.0",
    "d3": "^7.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "puppeteer": "^24.4.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/d3": "^7.4.3",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.10",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.2"
  }
}
